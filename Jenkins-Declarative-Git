pipeline{
    agent any
    environment{
    Path= "/maven-3/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git 'https://github.com/Vaishali0304/code.git'
            }
      }
        stage("Maven Build){
   steps{
       bat "mvn clean package"
   }
}
    }
}
